{
  "name": "import-sort-style-module-tsconfig",
  "version": "1.0.1",
  "description": "import-sort-style-module but treating tsconfig paths as local modules",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "tsc -b .",
    "build": "tsc -b .",
    "build:watch": "tsc -b . -w",
    "test": "mocha --require ts-node/register --recursive \"test/**/*.ts\"",
    "test:watch": "mocha -w --require ts-node/register --recursive \"test/**/*.ts\"",
    "lint": "eslint --ext ts --fix src"
  },
  "author": "Rustam SecondFry Gubaydullin <secondfry+npm@gmail.com> (https://github.com/secondfry)",
  "repository": "secondfry/import-sort-style-module-tsconfig",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chai": "^4.3.4",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-node": "^11.1.0",
    "import-sort-parser": "^6.0.0",
    "import-sort-style": "^6.0.0",
    "mocha": "^8.3.2",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "tsconfig-paths": "^3.9.0"
  }
}
